{
  "name": "react-dashboard",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:cc": "vite --force",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "biome check --fix src",
    "test": "vitest --ui",
    "coverage": "vitest run --coverage",
    "postinstall": "lefthook install",
    "commit": "git add -A && git commit --no-edit",
    "squash": "node ./squash.mjs && git commit --no-edit --no-verify"
  },
  "dependencies": {
    "@ant-design/charts": "^2.2.6",
    "@ant-design/cssinjs": "^1.23.0",
    "@tanstack/react-query": "^5.66.0",
    "@tanstack/react-query-devtools": "^5.65.1",
    "@tanstack/react-router": "^1.99.0",
    "@tanstack/router-devtools": "^1.99.0",
    "ahooks": "^3.8.4",
    "antd": "^5.23.3",
    "antd-enhanced": "^2.0.1",
    "react": "19.0.0",
    "react-auth-kit": "^3.1.3",
    "react-dom": "19.0.0",
    "react-icons": "^5.4.0",
    "react-spinners": "^0.15.0"
  },
  "devDependencies": {
    "@ant-design/v5-patch-for-react-19": "^1.0.3",
    "@biomejs/biome": "^1.9.4",
    "@faker-js/faker": "^9.4.0",
    "@tailwindcss/vite": "^4.0.3",
    "@tanstack/router-vite-plugin": "^1.99.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^22.13.0",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@vitejs/plugin-react-swc": "^3.7.2",
    "@vitest/coverage-v8": "^3.0.5",
    "@vitest/ui": "^3.0.5",
    "biome-extends": "^1.1.0",
    "git-cz": "^4.9.0",
    "happy-dom": "^16.8.1",
    "knip": "^5.43.6",
    "lefthook": "^1.10.10",
    "tailwindcss": "^4.0.3",
    "typescript": "^5.7.3",
    "vite": "^6.0.11",
    "vitest": "^3.0.5",
    "zx": "^8.3.2"
  },
  "engines": {
    "node": ">=22"
  },
  "trustedDependencies": ["@biomejs/biome", "@swc/core", "esbuild", "lefthook"]
