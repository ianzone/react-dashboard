{
  "name": "react-dashboard",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "bunx --bun vite --open",
    "dev:cc": "bunx --bun vite --force --open",
    "build": "tsc && bunx --bun vite build",
    "preview": "bunx --bun vite preview",
    "lint": "biome check --fix src",
    "test": "vitest run --coverage",
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui-host=0.0.0.0",
    "e2e:report": "playwright show-report e2e-out/playwright-report",
    "prepare": "lefthook install",
    "commit": "git add -A && git commit --no-edit",
    "squash": "bun scripts/squash.ts && git commit --no-edit --no-verify"
  },
  "dependencies": {
    "@ant-design/charts": "^2.2.6",
    "@ant-design/cssinjs": "^1.23.0",
    "@tanstack/react-query": "^5.66.7",
    "@tanstack/react-query-devtools": "^5.66.7",
    "@tanstack/react-router": "^1.106.0",
    "@tanstack/router-devtools": "^1.106.0",
    "ahooks": "^3.8.4",
    "antd": "^5.24.1",
    "antd-enhanced": "^2.0.2",
    "react": "19.0.0",
    "react-auth-kit": "^3.1.3",
    "react-dom": "19.0.0",
    "react-icons": "^5.5.0",
    "react-spinners": "^0.15.0"
  },
  "devDependencies": {
    "@ant-design/v5-patch-for-react-19": "^1.0.3",
    "@biomejs/biome": "^1.9.4",
    "@faker-js/faker": "^9.5.0",
    "@playwright/test": "^1.50.1",
    "@tailwindcss/vite": "^4.0.7",
    "@tanstack/router-vite-plugin": "^1.106.0",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^22.13.4",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react-swc": "^3.8.0",
    "@vitest/coverage-v8": "^3.0.6",
    "es-proj": "^1.5.0",
    "git-cz": "^4.9.0",
    "happy-dom": "^17.1.1",
    "knip": "^5.44.4",
    "lefthook": "^1.10.10",
    "tailwindcss": "^4.0.7",
    "typescript": "^5.7.3",
    "vite": "^6.1.1",
    "vitest": "^3.0.6"
  },
  "engines": {
    "node": ">=22"
  },
  "trustedDependencies": [
    "@biomejs/biome",
    "@swc/core",
    "antd-enhanced",
    "esbuild",
    "lefthook",
    "sharp"
  ]
}
